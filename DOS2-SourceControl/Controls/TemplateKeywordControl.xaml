<UserControl x:Class="LL.DOS2.SourceControl.Controls.TemplateKeywordControl"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:local="clr-namespace:LL.DOS2.SourceControl.Controls"
			 mc:Ignorable="d" 
			 d:DesignHeight="300" d:DesignWidth="300"
			 x:Name="TemplateKeywordMain"
			 >
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="150"/>
			<ColumnDefinition Width="*"/>
		</Grid.ColumnDefinitions>
		<TextBox Grid.Column="0" x:Name="KeywordNameBox" Text="{Binding KeywordName, ElementName=TemplateKeywordMain}" IsReadOnly="{Binding KeywordNameReadOnly, ElementName=TemplateKeywordMain}" Foreground="#FF415AF9" FontFamily="Neuton" FontWeight="Bold" />
		<TextBox Grid.Column="1" x:Name="KeywordValueBox" Text="{Binding KeywordValue, ElementName=TemplateKeywordMain}" IsReadOnly="{Binding KeywordValueReadOnly, ElementName=TemplateKeywordMain}" FontFamily="Neuton" />
	</Grid>
</UserControl>
